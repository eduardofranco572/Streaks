datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @id @default(autoincrement())
  name      String
  email     String   @unique
  password  String
}

model Contact {
  id         Int      @id @default(autoincrement())
  email      String
  externalId String
  createdAt  DateTime @default(now())
}
